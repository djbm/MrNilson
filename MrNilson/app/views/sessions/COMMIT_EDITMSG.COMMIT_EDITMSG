diff --git a/todo/app/models/list.rb b/todo/app/models/list.rb
new file mode 100644
index 0000000..5519131
--- /dev/null
+++ b/todo/app/models/list.rb
@@ -0,0 +1,12 @@
+class List < ActiveRecord::Base
+  
+  validates :name, :presence => true
+  validates_uniqueness_of :name, :on => :create, :message => "must be unique"
+  
+  has_many :tasks , :dependent => :destroy
+
+  def done_tasks
+     tasks.where(:done => true).order("updated_at DESC")
+  end
+  
+end
diff --git a/todo/app/models/task.rb b/todo/app/models/task.rb
new file mode 100644
index 0000000..8f773a8
--- /dev/null
+++ b/todo/app/models/task.rb
@@ -0,0 +1,8 @@
+class Task < ActiveRecord::Base
+    
+    belongs_to :list, :class_name => "List", :foreign_key => "list_id"
+    
+    validates :name, :presence => true
+
+
+end
